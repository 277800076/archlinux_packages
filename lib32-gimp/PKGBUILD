pkgname=lib32-gimp
pkgver=2.8.14
pkgrel32=1
pkgrel=1
pkgdesc="32bit gimp libraries"
arch=('x86_64')
url="http://www.gimp.org/"
depends=( 'lib32-glibc' 'lib32-cairo' 'lib32-gdk-pixbuf2' 'lib32-glib2' 'lib32-pango' 'gimp')
license=('GPL')
groups=lib32
source=(http://ftp.nluug.nl/os/Linux/distr/archlinux/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('fdb18a3b32657e75d19073d3cdcd7807')

package() {
	mkdir -p ${pkgdir}/usr/lib32
	cp -rPf ${srcdir}/usr/lib/*.so* ${pkgdir}/usr/lib32
}

